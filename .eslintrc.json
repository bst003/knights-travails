{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["main.js"],
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "operator-linebreak": "off",
        "prefer-object-spread": "off",
        "import/named": "off",
        "comma-dangle": "off",
        "import/prefer-default-export": "off",
        "consistent-return": "off",
        "prefer-destructuring": "off",
        "no-use-before-define": "off",
        "quotes": ["error", "double"],
        "indent": ["error", 4]
    }
}